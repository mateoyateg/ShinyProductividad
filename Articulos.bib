@article{Ind.184,
author={    Sergio Rios-Aguilar; Jordán Pascual-EspadaR; ubén González-Crespo},
title={nfc and cloud-based lightweight anonymous assessment mobile intelligent information system for higher education and recruitment competitions},
year={2015},

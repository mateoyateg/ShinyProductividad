@article{Ind.213,
author={LUIS ALEJANDRO MORALES MARIN; HOWARD WHEATER; KARL ERICH LINDENSCHMIDT; },
title={regional total phosphorus export modeling in the red deer river catchment using sparrow},
year={2014},
journal={GIIRA},
